{
    "name": "tomasvotruba/phpfwtrends-org",
    "license": "MIT",
    "description": "Micro site with stats of PHP framework downloads - raw Packagist data over guessing and emotions",
    "require": {
        "php": ">=8.0",
        "doctrine/cache": "^1.10",
        "guzzlehttp/guzzle": "^7.2",
        "j7mbo/twitter-api-php": "^1.0",
        "nette/finder": "^2.5",
        "nette/utils": "^3.2",
        "spatie/packagist-api": "^2.0.2",
        "symfony/asset": "^5.2",
        "symfony/cache": "^5.2",
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/flex": "^1.12",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/twig-bridge": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symplify/amnesia": "dev-master",
        "symplify/easy-hydrator": "dev-master",
        "symplify/package-builder": "dev-master",
        "symplify/php-config-printer": "dev-master",
        "symplify/smart-file-system": "dev-master",
        "symplify/symfony-static-dumper": "dev-master",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "cweagans/composer-patches": "dev-master as 1.7",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12.80",
        "phpunit/phpunit": "^9.0",
        "rector/rector": "^0.9",
        "spatie/http-status-check": "^3.3",
        "symfony/var-dumper": "^5.2",
        "symplify/coding-standard": "dev-master",
        "symplify/easy-coding-standard": "dev-master",
        "symplify/phpstan-extensions": "dev-master",
        "symplify/phpstan-rules": "dev-master",
        "symplify/vendor-patches": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\PhpFwTrends\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotruba\\PhpFwTrends\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify",
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-patches-skip-reporting": true,
        "patches": {
            "symplify/autowire-array-parameter": [
                "patches/symplify-autowire-array-parameter-src-dependencyinjection-compilerpass-autowirearrayparametercompilerpass-php.patch"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
